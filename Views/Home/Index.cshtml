                                                </li>
                                                <li>
                                                    <span class="glyphicon glyphicon-calendar" style="font-size:15px;" aria-hidden="true"></span>
                                                    <a href="#">@item.Date.ToShortDateString()</a>
                                                </li>
                                                @{
                                                    String myVenue1 = item.Venue;
                                                }
                                                @if (myVenue1 == "Online")
                                                {
                                                    <li>
                                                        <span class="glyphicon glyphicon-record" style="font-size:15px;" aria-hidden="true"></span>
                                                        <a href="#">@item.Venue</a>
                                                    </li>
                                                }
                                                else
                                                {
                                                    <li>
                                                        <span class="glyphicon glyphicon-map-marker" style="font-size:15px;" aria-hidden="true"></span>
                                                        <a href="#">@item.City</a>
                                                    </li>
                                                }
                                            </ul>
                                        </div>
                                        <div class="clearfix"></div>
                                        <div class="center-block">
                                            <div class="col-lg-1"></div>
                                            <a class="read hvr-shutter-in-horizontal col-lg-10 text-center" href="~/Home/Event?id=@item.Id">Get tickets</a>
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                    <div  style="margin-bottom:20px;"></div>
                                </div>

                                                    count++;
                                                    if(count == 6)
                                                    {
                                                        break;
                                                    }
                                                }
                                </div>
                            </div>
                        </div>
