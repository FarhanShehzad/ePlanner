                    <div id="myTabContent" class="tab-content">
                        <div role="tabpanel" class="tab-pane fade in active" id="expeditions" aria-labelledby="expeditions-tab">
                            <div class="section__content clearfix">
                                <!-- /card1 -->
                                @{ int count = 0; }
                                @foreach (var item in Model)
                                {
                                <div class="col-lg-4" style="max-width:360px; margin:10px; float:left; border: 1px solid blue; padding:0px;">
                                    <div>
                                        <a href="~/Home/Event?id=@item.Id" target="_blank" style="margin-bottom: 20px;">
                                            <img src="~/images/Events/@item.Image" style="width:100%;" alt="@item.Title" class="img-responsive" />
                                        </a>
                                    </div>
                                    <div class="clearfix"></div>
                                    <div style="margin:5px; margin-top:10px;">
                                        <span style="font-size:18px; margin:5px auto;"><a href="~/Home/Event?id=@item.Id" target="_blank" color="black">@item.Title</a></span>
                                        <span style="text-align:right; float:right;" class="pull-right">$@item.PriceOfTicket</span>
                                        <br />
                                        <div class="clearfix"></div>
                                        <div margin>
                                            <ul class="list-inline center-block text-center" style="margin-top:15px; margin-bottom:15px; border-top:0px; border-bottom:0px;">
